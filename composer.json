    "description": "Port of laravel factory" ,
    "type":        "yii2-extension" ,
    "keywords":    [ "yii2", "seed", "testing"] ,
    "license":     "MIT" ,
    "authors":     [
        {
            "name":  "insolita" ,
            "email": "webmaster100500@ya.ru"
        }
    ] ,
    "require":     {
        "php":           ">=7.0" ,
        "yiisoft/yii2":  "~2.0.10",
        "fzaninotto/faker": "~1.7"
